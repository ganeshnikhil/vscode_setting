{
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.3gpp": "default",
        "*.xz": "default",
        "*.out": "default",
        "*.pyc": "default",
        "*.jpg?sa=X&ved=2ahUKEwiZ0sL47I2AAxUTSWwGHR7XBIoQ_B16BAgCEAI": "default",
        "*.whl": "default",
        "*.heic": "default"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "C_Cpp.updateChannel": "Insiders",
    "kite.showWelcomeNotificationOnStartup": false,
    "files.autoSave": "afterDelay",
    "notebook.output.textLineLimit": 60,
    "editor.tabSize": 3,
    "workbench.colorTheme": "One Dark Pro Darker",
    "editor.fontFamily": "Menlo",
    // terminal font size 
    "terminal.integrated.fontSize": 13,
    //Default High Contrast
    //zoom levelof window
    "window.zoomLevel":2,
    "tabnine.experimentalAutoImports": true,
    "[python]": {
        "editor.formatOnType": true
    },
    "editor.foldingHighlight":true,

    "workbench.colorCustomizations": {
        "terminal.background": "#070707",
        "terminal.selectionBackground": "#E07c24",
        "terminal.selectionForeground": "#000000",
        "tab.activeBorder": "#FFFFFF",
        "tab.activeBackground": "#000000",
        "tab.activeBorderTop": "#FFFFFF",
        "tab.activeForeground":"#FFFFFF",
        "terminal.foreground": "#00FF00",

    },
    "editor.tokenColorCustomizations": {
        "comments": "#1f8e22"

    },
    "todohighlight.keywords": [{
       "text":"#",
       "backgroundColor": "#000000",
       "color":"#FF0000"

    },
    {"text":"[*]",
    "backgroundColor": "#000000",
    "color":"#1f8e22"

    },
{
    //highlits the given text whenever we write it in vscode files
    "text":"ganesh nikhil",
    "backgroundColor": "#1f8e22",
    "color":"#FF0000"
}]
,

